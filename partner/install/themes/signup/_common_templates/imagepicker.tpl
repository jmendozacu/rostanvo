<!-- imagepicker -->
<div class="PopupWinTopLeft"><div class="PopupWinTopRight"><div class="PopupWinTop"></div></div></div>
<div class="PopupWinLeft"><div class="PopupWinRight">
<div class="PopupWinMain">

	<table border=0 cellspacing=0 cellpadding="3">
	<tr>
	  <td valign="top" colspan="3">##Upload image##</td>
	</tr><tr>
	  <td valign="top">{widget id="FileUpload"}</td>
	  <td>&nbsp;</td>
	  <td>{widget id="UploadApplyButton"}</td>
	</tr><tr>
	  <td valign="top" align="left" colspan="3">##File on the Internet (enter image URL)##</td>
	</tr><tr>
	  <td valign="top" align="left" >{widget id="FileUrl"}</td>
	  <td>&nbsp;</td>
	  <td>{widget id="UrlApplyButton"}</td>
	</table>
	
</div></div></div>
<div class="PopupWinBottomLeft"><div class="PopupWinBottomRight"><div class="PopupWinBottom"></div></div></div>
