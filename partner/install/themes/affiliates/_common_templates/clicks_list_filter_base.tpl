 <!-- clicks_list_filter_base -->
<div class="RawClicksFilter">
		<div class="ColumnFieldset">
				<fieldset class="Filter FilterChannel">
                    <legend>##Channel##</legend>
                    <div class="Resize">
                    {widget id="channel"}
                    </div>
   				</fieldset>    
    
  				<fieldset class="Filter FilterCampaign">
    			<legend>##Campaign##</legend>
    			<div class="Resize">
    			{widget id="campaignid"}
    			</div>
    			</fieldset>
    
		</div>
	<fieldset class="Filter FilterDate">
		<legend>##Date##</legend>
		<div class="Resize">
			{widget id="datetime"}
		</div>
	</fieldset>  
 
	<fieldset class="Filter">
		<legend>##Type##</legend>
		<div class="Resize">
			{widget id="rtype"}
		</div>
	</fieldset>
	<fieldset class="Filter">
		<legend>##Custom##</legend>
		<div class="Resize">
			{widget id="custom"}
		</div>
	</fieldset>
</div>
<div class="clear"></div>
    
