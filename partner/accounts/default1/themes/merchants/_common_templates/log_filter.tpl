<!-- log_filter -->
    
    	<div class="LogFilter">
       		
       		<fieldset class="Filter">
            <legend>##Date created##</legend>
            <div class="Resize">
            {widget id="created"}
            </div>
       		</fieldset>
        
        	<fieldset class="Filter">
            <legend>##Time##</legend>
            <div class="Resize">
            {widget id="time_created"}{widget id="time"}
            </div>
        	</fieldset>
        
        	<fieldset class="Filter">        	
            <legend>##Log level##</legend>
            <div class="Resize">
            {widget id="level"}
            </div>
        	</fieldset>
        
       		<fieldset class="Filter">
            <legend>##Custom##</legend>
            <div class="Resize">
            {widget id="custom"}
            </div>
        	</fieldset>
        
        </div>
        
        <div style="clear: both;"></div>
    
